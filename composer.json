{
    "name": "jeromefitzpatrick/laravel-starting-point",
    "description": "Laravel Starting Point provides an 80% use case starting point for API driven applications",
    "keywords": ["laravel", "starting point", "bootstrap"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jeromefitzpatrick/laravel-starting-point/issues",
        "source": "https://github.com/jeromefitzpatrick/laravel-starting-point"
    },
    "authors": [
        {
            "name": "Jerome Fitzpatrick",
            "email": "jerome@jeromefitzpatrick.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "spatie/laravel-fractal": "^5.8|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0"
    },
    "suggest": {

    },
    "autoload": {
        "psr-4": {
            "JeromeFitzpatrick\\StartingPoint\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeromeFitzpatrick\\StartingPoint\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JeromeFitzpatrick\\StartingPoint\\StartingPointServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
