{
    "name": "jeromefitzpatrick/laravel-starting-point",
    "description": "Laravel Starting Point provides an 80% use case starting point for API driven applications",
    "keywords": ["laravel", "starting point", "bootstrap"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jeromefitzpatrick/laravel-starting-point/issues",
        "source": "https://github.com/jeromefitzpatrick/laravel-starting-point"
    },
    "authors": [
        {
            "name": "Jerome Fitzpatrick",
            "email": "jerome@jeromefitzpatrick.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/log": "^6.0|^7.0|^8.0",
        "illuminate/notifications": "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/view": "^6.0|^7.0|^8.0",
        "nesbot/carbon": "^2.0",
        "spatie/laravel-fractal": "^5.8",
        "symfony/http-kernel": "^4.3|^5.0",
        "symfony/intl": "^4.3|^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "suggest": {

    },
    "autoload": {
        "psr-4": {
            "JeromeFitzpatrick\\StartingPoint\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeromeFitzpatrick\\StartingPoint\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "StartingPointServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
